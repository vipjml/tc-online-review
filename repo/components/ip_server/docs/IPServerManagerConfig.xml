<CMConfig>
  <Config name="com.topcoder.processor.ipserver">
    <!--Server names, required-->
    <Property name="servers">
      <Value>server1</Value>
      <Value>server2</Value>
    </Property>

    <!--Listen IP address, optional-->
    <Property name="server1_address">
      <Value>192.168.0.2</Value>
    </Property>
    <!--Listen port, required-->
    <Property name="server1_port">
      <Value>8080</Value>
    </Property>
    <!--Maximum connections, optional-->
    <Property name="server1_max_connections">
      <Value>5000</Value>
    </Property>
    <!--Namespace used to create message factory, required-->
    <Property name="server1_message_factory_namespace">
      <Value>com.topcoder.processor.ipserver.message</Value>
    </Property>
    <!--Start upon startup, optional-->
    <Property name="server1_started">
      <Value>true</Value>
    </Property>
    <!--Number of backlog, optional-->
    <Property name="server1_backlog">
      <Value>100</Value>
    </Property>
    <!--Names of handlers, required-->
    <Property name="server1_handlers">
      <Value>handler1</Value>
      <Value>handler2</Value>
    </Property>

    <!--Class name of handler, required-->
    <Property name="server1_handler1_class">
      <Value>com.topcoder.app.Server1Handler1</Value>
    </Property>
    <!--Maximum requests, optional-->
    <Property name="server1_handler1_max_requests">
      <Value>10</Value>
    </Property>

    <!--Class name of handler, required-->
    <Property name="server1_handler2_class">
      <Value>com.topcoder.app.Server1Handler2</Value>
    </Property>


    <!--Listen port, required-->
    <Property name="server2_port">
      <Value>1433</Value>
    </Property>
    <!--Namespace used to create message factory, required-->
    <Property name="server2_message_factory_namespace">
      <Value>com.topcoder.app.custommessage</Value>
    </Property>
    <!--Names of handlers, required-->
    <Property name="server2_handlers">
      <Value>handler1</Value>
    </Property>

    <!--Class name of handler, required-->
    <Property name="server2_handler1_class">
      <Value>com.topcoder.app.Server2Handler1</Value>
    </Property>
  </Config>
</CMConfig>