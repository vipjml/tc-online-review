                        <Value>com.topcoder.search.builder.filter.NullFilter</Value>
                    </Property>
                    <Property name="className">
                        <Value>com.topcoder.search.builder.database.NullFragmentBuilder</Value>
                    </Property>
                </Property>

                <Property name="ninth">
